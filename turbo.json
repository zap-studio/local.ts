{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "outputs": ["dist/**"]
    },
    "check": {
      "dependsOn": ["^check"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "format": {
      "cache": false
    },
    "lint": {},
    "test": {
      "cache": false
    },
    "test:coverage": {
      "cache": false
    },
    "test:watch": {
      "cache": false
    }
  }
}
